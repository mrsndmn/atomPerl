// Поместите параметры в этот файл, чтобы перезаписать параметры по умолчанию и пользовательские параметры.
{
    "editor.minimap.enabled": true,
    "code-runner.defaultLanguage": "perl",
    "files.associations": {
        "*.tt": "html",
        "*.js": "html",
        "scc": "html",
        "*.xs": "c",
        "*.inc": "cpp",
        "system_error": "cpp",
        "type_traits": "cpp"
    }
}